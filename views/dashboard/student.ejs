<h1>
    Welcome,
    <%= user.name %>
    (Student)
</h1>

<div class="board">
    <%- include('_announcement.ejs') %>
    <div class="card">
        <h2>Your Enrolled Courses</h2>
        <% if (enrolledCourses.length > 0) { %>
        <ul>
            <% enrolledCourses.forEach(function(course) { %>
            <li>
                <%= course.name %>
            </li>
            <% }) %>
        </ul>
        <% } else { %>
        <p>No courses here</p>
        <% } %>
        <%- contentFor('title') %>Dashboard
    </div>
</div>
