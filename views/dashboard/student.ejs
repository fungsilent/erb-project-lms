<!DOCTYPE html>
<html>
<head>
    <title>Student Dashboard</title>
</head>
<body>
    <h1>Welcome, <%= user.name %> (Student)</h1>
    <h2>Your Enrolled Courses</h2>
    <% if (enrolledCourses.length > 0) { %>
        <ul>
            <% enrolledCourses.forEach(function(course) { %>
                <li>
                    <%= course.name %>
                </li>
            <% }) %>
        </ul>
    <% } else { %>
        <p>No courses here</p>
    <% } %>
    <a href="/logout">Logout</a>
</body>
</html>
